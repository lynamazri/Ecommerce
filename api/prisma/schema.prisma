generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users{
  userId String @id @default(uuid())
   username        String @unique      
  firstName      String
  lastName      String
  email     String    @unique
  password  String
  birthDate DateTime @db.Date
  gender String
adresses Address[]
  registrationDate DateTime  @default(now())
  refreshToken String?

}


model Address{
  id       String  @id @default(uuid())
  user   Users @relation(fields: [userId], references: [userId])
  userId String
  street String
  city String
  state String
  zip Int
}
